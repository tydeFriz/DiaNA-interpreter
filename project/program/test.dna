USE ./program/or.dna AS test


LABEL Start
    RUN test_or

LABEL test_or
    GLUE test.true true_case
    GLUE test.false false_case
    COPY test.bool.False
    COPY test.bool.False
    GLUE test.param_a test.bool.False
    GLUE test.param_b test.bool.False
    RUN test.or

LABEL cleanup
    CUT test.param_a DOWN
    CUT test.param_b DOWN
    CUT test.true DOWN
    CUT test.false DOWN

LABEL true_case
    KILL F
    RUN cleanup

LABEL false_case
    KILL T
    RUN cleanup

LABEL T

LABEL F
